{
  "compilerOptions": {
    "declaration": false,
    "declarationMap": false,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },

  "exclude": [
    "node_modules"
  ],
  "extends": "@nebulatics/typescript-config/nextjs.json",
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "next-env.d.ts",
    "next.config.ts",
    ".next/types/**/*.ts"
  ]
}
