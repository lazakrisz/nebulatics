{
  "name": "nebulatics",
  "version": "0.0.0",
  "private": false,
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "browser": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": "./dist/index.mjs"
    },
    "./api": {
      "import": {
        "types": "./dist/api/index.d.mts",
        "default": "./dist/api/index.mjs"
      },
      "require": {
        "types": "./dist/api/index.d.cts",
        "default": "./dist/api/index.cjs"
      }
    },
    "./dashboard": {
      "react-server": {
        "types": "./dist/dashboard/index.d.mts",
        "default": "./dist/dashboard/index.mjs"
      },
      "import": {
        "types": "./dist/dashboard/index.d.mts",
        "default": "./dist/dashboard/index.mjs"
      },
      "require": {
        "types": "./dist/dashboard/index.d.cts",
        "default": "./dist/dashboard/index.cjs"
      }
    },
    "./next": {
      "import": {
        "types": "./dist/next/index.d.mts",
        "default": "./dist/next/index.mjs"
      },
      "require": {
        "types": "./dist/next/index.d.cts",
        "default": "./dist/next/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch ./src"
  },
  "dependencies": {
    "better-call": "^1.1.3",
    "client-only": "^0.0.1",
    "fumadb": "^0.2.1",
    "server-only": "^0.0.1",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@nebulatics/typescript-config": "workspace:*",
    "@types/react": "^19.2.7",
    "tsdown": "^0.16.8",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "next": "^14.0.0 || ^15.0.0 || ^16.0.0",
    "react": "^18.0.0 || ^19.0.0"
  }
}
